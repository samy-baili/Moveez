<resources>

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="MovieCardView">
        <attr name="title" format="string" />
        <attr name="overview" format="string" />
        <attr name="vote_average" format="float" />
    </declare-styleable>

    <declare-styleable name="IconicLabelView">
        <attr name="iconFont" format="string" />
        <attr name="label" format="string" />
        <attr name="iconSize" format="dimension" />
        <attr name="labelSize" format="dimension" />
    </declare-styleable>

</resources>
